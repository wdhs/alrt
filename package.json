  "lint-staged": {
    "*.(ts|tsx)": [
      "npm run lint:fix",
      "npm run pretty"
    ],
    "*.css": [
      "npm run pretty"
    ],
    "*.json": [
      "npm run pretty"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "overrides": {
    "tinycolor2": "1.4.2",
